# Environment Variables (Generated by Ansible)
GITHUB_USERNAME={{ github_username }}
GITHUB_REPO={{ github_repo }}

# Database (RDS)
DB_HOST={{ db_host }}
DB_NAME={{ db_name }}
DB_USER={{ db_user }}
DB_PASSWORD={{ db_password }}

# Redis
REDIS_PASSWORD={{ redis_password }}

# Application
APP_SECRET_KEY={{ app_secret_key }}
